{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "SBOX",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.sbox.nex",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "bannerPath": "icons/banner/mini.jpg",
          "dialogImagePath": "icons/banner/big.jpg"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "minHeight": 540,
        "resizable": true,
        "title": "SBOX",
        "minWidth": 960,
        "visible": false,
        "center": true,
        "theme":"Dark",
        "transparent": true,
        "titleBarStyle":"Transparent" 
      },
      {
       "width": 600,
       "height": 360,
       "resizable": false,
       "decorations": false,
       "url": "index.html",
       "title": "LOADING",
       "label": "splashscreen",
       "center": true
      }
    ]
  }
}